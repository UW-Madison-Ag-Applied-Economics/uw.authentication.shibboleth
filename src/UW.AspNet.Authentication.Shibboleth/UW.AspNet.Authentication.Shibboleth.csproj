<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net45</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>UW.Authentication.AspNet.snk</AssemblyOriginatorKeyFile>
    <RootNamespace>UW.AspNet.Authentication</RootNamespace>
    <Description>Library used to handle UW's Shibboleth authentication in ASP.NET web applications.</Description>
    <Company>AAE IT Services Center</Company>
    <Authors>Eric Dieckman</Authors>
    <Version>2.0.0-beta1</Version>
    <PackageLicenseFile>license.txt</PackageLicenseFile>
    <PackageProjectUrl>https://git.aae.wisc.edu/lib/uw.authentication.shibboleth</PackageProjectUrl>
    <RepositoryUrl>https://git.aae.wisc.edu/lib/uw.authentication.shibboleth.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\license.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\UW.Shibboleth\UW.Shibboleth.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="System.Web" />
  </ItemGroup>

</Project>
