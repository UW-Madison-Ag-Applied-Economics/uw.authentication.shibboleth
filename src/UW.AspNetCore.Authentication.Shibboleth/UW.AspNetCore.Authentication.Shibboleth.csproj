<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netcoreapp3.1;net5.0;net6.0</TargetFrameworks>
    <Authors>Eric Dieckman</Authors>
    <Company>AAE IT Services Center</Company>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Library for handling UW's Shibboleth authentication in ASP.Net web applications</Description>
    <RepositoryUrl>https://git.aae.wisc.edu/lib/uw.authentication.shibboleth.git</RepositoryUrl>
    <Version>6.1.0-beta1</Version>
    <RootNamespace>UW.AspNetCore.Authentication</RootNamespace>
    <PackageProjectUrl>https://git.aae.wisc.edu/lib/uw.authentication.shibboleth</PackageProjectUrl>
    <PackageLicenseFile>license.txt</PackageLicenseFile>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\license.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>


  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>


  <ItemGroup>
    <PackageReference Include="UW.Shibboleth" Version="5.0.0" />
  </ItemGroup>

</Project>
